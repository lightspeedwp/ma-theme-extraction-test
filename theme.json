        },
        {
          "slug": "neutral-300",
          "color": "#CCCCD5",
          "name": "Neutral 300"
        },
        {
          "slug": "neutral-400",
          "color": "#B7B7BE",
          "name": "Neutral 400"
        },
        {
          "slug": "neutral-500",
          "color": "#A4A4A5",
          "name": "Neutral 500"
        },
        {
          "slug": "neutral-600",
          "color": "#878788",
          "name": "Neutral 600"
        },
        {
          "slug": "neutral-700",
          "color": "#6B6B6C",
          "name": "Neutral 700"
        },
        {
          "slug": "neutral-800",
          "color": "#4F4F4F",
          "name": "Neutral 800"
        },
        {
          "slug": "neutral-900",
          "color": "#323232",
          "name": "Neutral 900"
        },
        {
          "slug": "cta-light",
          "color": "#FFEDE7",
          "name": "CTA Light"
        },
        {
          "slug": "cta-core",
          "color": "#E85E2E",
          "name": "CTA Core"
        },
        {
          "slug": "cta-dark",
          "color": "#C75128",
          "name": "CTA Dark"
        },
        {
          "slug": "brand-light",
          "color": "#E2F4F9",
          "name": "Brand Light"
        },
        {
          "slug": "brand-core",
          "color": "#00B2D5",
          "name": "Brand Core"
        },
        {
          "slug": "brand-dark",
