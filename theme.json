      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": true,
      "text": true,
      "palette": [
        {
          "slug": "base",
          "color": "#FFFFFF",
          "name": "Base"
        },
        {
          "slug": "contrast",
          "color": "#111111",
          "name": "Contrast"
        },
        {
          "slug": "neutral-100",
          "color": "#F9F9F9",
          "name": "Neutral 100"
        },
        {
          "slug": "neutral-200",
          "color": "#E3E3EA",
