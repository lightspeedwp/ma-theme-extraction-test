{
  "name": "lightspeedwp/ma-theme",
  "description": "A block theme for Medical Academic and thier 4 Brands",
  "type": "wordpress-theme",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "LightSpeedWP",
      "homepage": "https://lightspeedwp.agency"
    }
  ],
  "support": {
    "issues": "https://github.com/LightSpeedWP/ma-theme/issues",
    "source": "https://github.com/LightSpeedWP/ma-theme"
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "phpunit/phpunit": "^9.0",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "scripts": {
    "lint": "phpcs",
    "lint:fix": "phpcbf",
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}