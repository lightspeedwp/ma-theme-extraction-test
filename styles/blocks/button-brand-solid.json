{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "title": "Brand Solid",
  "blockTypes": [
    "core/button"
  ],
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--brand-core)",
      "text": "var(--wp--preset--color--base)"
    },
    ":hover": {
      "color": {
        "background": "var(--wp--preset--color--brand-dark)",
        "text": "var(--wp--preset--color--base)"
      }
    }
  }
}